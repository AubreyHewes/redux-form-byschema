{
  "name": "@hewes/redux-form-byschema",
  "version": "0.0.0-alpha22",
  "description": "Generate a redux-form from a JSON schema",
  "author": "Aubrey Hewes <https://github.com/AubreyHewes>",
  "license": "MIT",
  "homepage": "https://github.com/AubreyHewes/redux-form-byschema#README.md",
  "bugs": {
    "url": "https://github.com/AubreyHewes/redux-form-byschema/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AubreyHewes/redux-form-byschema.git"
  },
  "keywords": [
    "redux-form",
    "json",
    "schema",
    "jsonschema",
    "react",
    "form",
    "redux"
  ],
  "main": "dist/index.js",
  "scripts": {
    "test": "ava",
    "lint": "eslint .",
    "lint:fix": "yarn run lint --fix",
    "build": "rimraf ./dist && babel ./src --out-dir ./dist",
    "prepublish": "yarn run build"
  },
  "engine": {
    "node": ">=8",
    "npm": ">=6",
    "yarn": ">=1"
  },
  "peerDependencies": {
    "ajv": "^6.4.0",
    "immutable": "^3.8.2",
    "lodash": "^4.17.5",
    "prop-types": "^15.5.7",
    "react": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0"
  },
  "dependencies": {
    "invariant": "^2.2.1"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^4.0.0",
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-do-expressions": "^7.5.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@hewes/eslint-config": "^1.1.3",
    "ajv": "^6.4.0",
    "ava": "^2.2.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-istanbul": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-plugin-ava": "^7.1.0",
    "husky": "^3.0.3",
    "immutable": "^3.8.2",
    "lodash": "^4.17.11",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.2.0",
    "react-dom": "^15.3.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "rimraf": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "@hewes/eslint-config",
    "globals": {
      "__DEV__": false,
      "__DEBUG__": false
    },
    "env": {
      "browser": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "eslintIgnore": [
    "coverage/**",
    "node_modules/**",
    "dist/**",
    "*.spec.js"
  ],
  "prettier": {
    "printWidth": 120
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-function-bind",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-logical-assignment-operators",
      [
        "@babel/plugin-proposal-optional-chaining",
        {
          "loose": false
        }
      ],
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "minimal"
        }
      ],
      [
        "@babel/plugin-proposal-nullish-coalescing-operator",
        {
          "loose": false
        }
      ],
      "@babel/plugin-proposal-do-expressions",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      "@babel/plugin-proposal-json-strings"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "ava": {
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "plugins": [],
        "presets": [
          "@ava/stage-4"
        ]
      }
    }
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint",
      "pre-push": "yarn run test"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "directories": {
    "lib": "dist",
    "test": "test"
  }
}
